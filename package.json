{
  "name": "vsnet-auth",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon ./core/server.js",
    "dev": "pm2 start ecosystem.config.js --no-daemon",
    "prod": "pm2 start ecosystem.config.js --env production",
    "restart": "pm2 restart all",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "list": "pm2 list",
    "clean": "rm -rf node_modules && npm cache verify && npm i",
    "setup": "npm i -g pm2@latest",
    "db_dev": "cockroach start --insecure --store=/makeshift-data --host=localhost",
    "lint": "./node_modules/.bin/eslint core/**",
    "nsp": "nsp check --output summary",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "jsonwebtoken": "^7.3.0",
    "mongodb": "^2.2.25",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "sequelize": "^4.5.0",
    "sequelize-cockroachdb": "^1.0.2",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "nodemon": "^1.11.0"
  }
}
